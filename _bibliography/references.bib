@article{tojal:2006arquitectura,
  title = {Arquitectura doméstica em Faro na segunda metade do século XIX: normas e práticas},
  shorttitle = {Arquitectura doméstica em Faro na segunda metade do século XIX},
  author = {Tojal, Alexandre Arménio Maia},
