@incollection{rocha:2018paisagem,
  title = {Paisagem urbana, morfologia e arquitetura doméstica portuense: a urbanização do Campo do Cirne no século XIX},
  shorttitle = {Paisagem urbana, morfologia e arquitetura doméstica portuense},
