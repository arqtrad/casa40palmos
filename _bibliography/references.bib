@thesis{rodrigues:2007patrimonio,
  type = {Dissertação (doutoramento)},
  title = {Património, território e topologia do lugar, no Brasil, no estado de Goiás : contributo para a metodologia projectural do espaço urbano e estruturas de suporte em arquitectura},
