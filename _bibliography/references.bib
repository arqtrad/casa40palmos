  institution = {{Faculdade de Arquitectura, Universidade Técnica de Lisboa}},
  location = {{Lisboa}},
  url = {http://hdl.handle.net/10400.5/1456},
  urldate = {2013-10-06},
  abstract = {Esta Dissertação de Doutoramento em Arquitectura tem como objectivo sistematizar uma investigação científica na área disciplinar da Arquitectura, segundo as estratégias emergentes do processo projectual em Arquitectura, tendo como campo restrito temático o Território, a Topologia do Lugar e o seu Edificado. Objectivámos o papel da metodologia projectual com um propósito de evidenciar as estratégias topológicas que estão associadas ao acto de projectual do edificado, do lugar, e do território segundo as suas componentes referenciais. A identificação das componentes referenciais e contextuais permitiram a constituição dum valor patrimonial.  Essas componentes possibilitaram, ainda, a constituição dum processo de formação emocional e de decisão que estão associados aos sistemas de orientação espacial, fenomenológicos, sensoriais, da percepção e da memória referencial, obtidas pelas formas de mediatização, de interpretação e síntese.  A metodologia usada teve como caso de estudo a compreensão de sete lugares, fundados no período aurífero, no século XVIII, da antiga Capitania de Goiás, no centro-oeste do Brasil constituindo a base metodológica na formação do território, na estruturação do lugar e na composição do seu edificado.},
  keywords = {Heritage,Methodology,Metodologia,Património,Percepção,Projecto,Razão,Sense,Topologia,Topology},
  langid = {portuguese},
  type = {Dissertação (doutoramento)}
}

@thesis{rodrigues:2010faro,
  title = {Faro na época Moderna: do urbanismo à arquitectura},
  shorttitle = {Faro na época Moderna},
  author = {Rodrigues, Tânia},
  date = {2010},
  institution = {{Universidade do Algarve, Faculdade de Ciências Humanas e Sociais}},
  location = {{Faro}},
  url = {https://sapientia.ualg.pt/handle/10400.1/1744},
  urldate = {2020-06-02},
  abstract = {Tendo como objectivo o estudo do espaço urbano de Faro a partir da análise de  elementos específicos – o quarteirão, o lote, o conjunto edificado e a arquitectura  corrente - no período do Antigo Regime, desenvolvemos o nosso trabalho tendo como  base a imagem actual da cidade e os dados recolhidos, essencialmente, num tipo de  fontes especificas - tombos de propriedades imóveis - do Concelho e da Misericórdia.  O estudo desenvolve-se em três partes: a primeira, de contextualização do objecto  de estudo. Resume, em traços gerais, a evolução urbana do núcleo desde a sua génese  até ao final do século XVIII; a segunda, também numa linha de contextualização,  apresenta, analisa e interpreta as fontes primárias. Focando as potencialidades e as  limitações deste tipo de documentação e a sua dimensão geográfica; e a terceira,  concretiza a interpretação dos dados recolhidos confrontando, sempre que possível, com  o existente.  Neste contexto, identificaram-se os diferentes tipos de quarteirão e de lote e  esboçou-se a sua evolução morfológica; caracterizou-se o conjunto edificado; e, por  último, indicaram-se diferentes tipos de construções entre os quais se destacou a  morada de casa corrente, possibilitando a identificação e caracterização das diferentes  tipologias existentes em Faro.},
  annotation = {ZSCC: 0000007  Accepted: 2012-10-12T17:51:12Z},
  langid = {portuguese},
  type = {Dissertação (Mestrado em História da Arte)}
}

